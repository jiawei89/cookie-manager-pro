{
  "manifest_version": 3,
  "name": "Cookie Manager Pro",
  "version": "1.0.0",
  "description": "强大的Cookie管理器 - 添加、删除、编辑、搜索、锁定和屏蔽cookies",
  "permissions": [
    "cookies",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "js/background.js"
  }
}
